@model MVCSistemaWebAlumnos.Models.Alumno

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detalle Alumno</title>
</head>
<body>
    <div>
        <h2>Alumno: @Model.Apellido, @Model.Nombre</h2>
